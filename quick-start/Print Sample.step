els\"\n\t\t\t\t\t\t}\n\t\t\t\t\t],\n\t\t\t\t\t\"visible\": \"\"\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\t\"id\": \"rownum\",\n\t\t\t\t\t\"type\": \"checkbox\",\n\t\t\t\t\t\"label\": \"Display row number\",\n\t\t\t\t\t\"visible\": \"\"\n\t\t\t\t}\n\t\t\t]\n\t\t}\n\t],\n\t\"syntaxversion\": \"1.3.0\",\n\t\"values\": {\n\t\t\"inputtable1\": {\n\t\t\t\"library\": \"\",\n\t\t\t\"table\": \"\"\n\t\t},\n\t\t\"varlist\": [],\n\t\t\"numrows\": 20,\n\t\t\"selection\": {\n\t\t\t\"value\": \"FIRSTN\",\n\t\t\t\"label\": \"First N rows\"\n\t\t},\n\t\t\"rowcolor\": \"C7E3F9\",\n\t\t\"colhead\": {\n\t\t\t\"value\": \"0\",\n\t\t\t\"label\": \"Column names\"\n\t\t},\n\t\t\"rownum\": true\n\t}\n}","templates":{"SAS":"/* SAS templated code goes here */\n\n%macro printsample;\n\nproc sql noprint;\n    select count(*) format=comma20. into :N from &inputtable1;\nquit;\n\n%if &selection=FIRSTN %then %do;\n    title1 \"Sample from &inputtable1\";\n    title2 height=3 \"First &numrows of &N Rows\";\n    data sample;\n        set &inputtable1(obs=&numrows);\n    run;\n%end;\n\n%else %if &selection=RANDOM %then %do;\n    title1 \"Sample from &inputtable1\";\n    title2 height=3 \"Random Sample &numrows of &N Rows\";\n\n    proc surveyselect data=&inputtable1 \n                      method=srs n=&numrows\n                      out=sample noprint;\n    run;   \n%end;  \n\nfootnote height=3 \"Created %sysfunc(today(),nldatew.) at %sysfunc(time(), nltime.)\";\n\n%if &colhead=0 %then %do;\n    options nolabel;\n%end;\n\nproc report data=sample; \n    column \n        %if &rownum=1 %then %do;\n            Row\n        %end;\n        &varlist;\n    %do i=1 %to &varlist_count;\n        define &&varlist_&i._name / display;\n    %end;\n    compute &varlist_1_name;\n        count+1;\n        if mod(count,2) then do;\n            call define(_row_, \"style\", \"style=[background=#&rowcolor]\");\n        end;\n    endcomp;\n    %if &rownum=1 %then %do;\n        define Row / computed;\n        compute Row;\n            tempobs+1;\n            Row=tempobs;\n        endcomp;\n    %end;\nrun;\n\n%if &colhead=0 %then %do;\n    options label;\n%end;\ntitle;\n%mend;\n%printsample; "}}